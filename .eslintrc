{
  "rules": {
    "indent": [
      2,
      2
    ],
    "quotes": [
      2,
      "single"
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "semi": 0,
    "no-plusplus": 2,
    "no-unused-vars": 1,
    "no-bitwise": 2,
    "no-eval": 2,
    "curly": 2,
    "eqeqeq": 2,
    "guard-for-in": 2,
    "no-lone-blocks": 2,
    "no-multi-spaces": 2,
    "no-void": 2,
    "no-trailing-spaces": 2,
    "no-array-constructor": 2,
    "no-new-object": 2,
    "prefer-const": 2,
    "no-console": 1
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "extends": "eslint:recommended"
}
